.m {
    width: 1200px;
    margin: 0 auto;
}
body {
    width: 100%;
    height: 100%;
    border-top: 1px solid #fff;
    background-size: 100%;
    background: url(../img/downloadbg.jpg) no-repeat;
}
.fullpage {
    margin-bottom: 70px;
}
.fullpage h4 {
    margin-top: 10px;
}
.fullpage h4 a {
    color: aliceblue;
    font-size: 16px;
}
.fullpage h2 {
    font-size: 50px;
    color: #fff33a;
    text-align: right;
}
.fullpage h3 {
    margin-top: 26px;
    font-size: 20px;
    color: #fff;
    font-weight: 400;
    text-align: right;
    margin-bottom: 80px;
}
.app_main {
    margin-top: 300px;
}
.app_left .img {
    position: relative;
}
.app_left .img img:nth-child(1) {
    opacity: 0;
}
.app_left .img img:nth-child(2) {
    position: absolute;
    top: 0px;
    left: 0;
    opacity: 0;
}
.app_left .img img:nth-child(3) {
    position: absolute;
    top: 0px;
    left: 0;
    opacity: 0;
}
.app_left .img>div {
    position: absolute;
    width: 0px;
    height: 100px;
    border-radius: 50%;
    border-top: 2px dashed #fff;
    transform: rotate(-20deg);
    text-align: right;
}
.app_left .img>div span {
    width: 0px;
    height: 35px;
    position: absolute;
    transform: rotate(20deg);
}
.app_left .img>div:nth-child(4) {
    top: -100px;
    left: 360px;
}
.app_left .img>div:nth-child(4) span {
    top: 30px;
    right: -125px;
    background: url(../img/dingyue.png) no-repeat;
}
.app_left .img>div:nth-child(5) {
    top: -15px;
    left: 494px;
}
.app_left .img>div:nth-child(5) span {
    top: 30px;
    right: -125px;
    background: url(../img/zhuanding1.png) no-repeat;
}
.app_left .img>div:nth-child(6) {
    top: 75px;
    left: 622px;
}
.app_left .img>div:nth-child(6) span {
    top: 30px;
    right: -145px;
    background: url(../img/hlsj.png) no-repeat;
}
.app_right {
    text-align: right;
    margin-top: -120px;
}
.app_right p {
    margin-top: 20px;
    color: #fff;
    font-size: 16px;
}
.app_right .phone {
    display: block;
    width: 200px;
    height: 52px;
    text-align: center;
    line-height: 52px;
    background-color: #f68426;
    border-radius: 26px;
    margin-left: 258px;
}
.app_right .phone:nth-child(5) {
    margin-top: 50px;
}
.app_right .phone:nth-child(6) {
    margin-top: 20px;
}
.app_right .phone span {
    color: #fff;
    margin-left: 10px;
}
/* 动画部分 */

@keyframes late {
    from {
        opacity: 0;
    }
    to {
        opacity: 1;
    }
}
.app_left .img img:nth-child(1) {
    animation: late 0.8s linear forwards;
}
@keyframes late1 {
    0% {
        opacity: 0;
        top: 0px;
    }
    80% {
        opacity: 1;
    }
    100% {
        opacity: 1;
        top: -90px;
    }
}
.app_left .img img:nth-child(2) {
    animation: late1 1s 0.8s linear forwards;
}
@keyframes late2 {
    0% {
        opacity: 0;
        top: 0px;
    }
    80% {
        opacity: 1;
    }
    100% {
        opacity: 1;
        top: -150px;
    }
}
.app_left .img img:nth-child(3) {
    animation: late2 0.95s 0.8s linear forwards;
}
@keyframes show {
    from {
        width: 0px;
    }
    to {
        width: 150px;
    }
}
.app_left .img>div:nth-child(4) {
    animation: show 0.5s 1.6s linear forwards;
}
.app_left .img>div:nth-child(5) {
    animation: show 0.5s 2.6s linear forwards;
}
.app_left .img>div:nth-child(6) {
    animation: show 0.5s 3.6s linear forwards;
}
.app_left .img>div:nth-child(4) span {
    animation: show 0.5s 2.1s linear forwards;
}
.app_left .img>div:nth-child(5) span {
    animation: show 0.5s 3.1s linear forwards;
}
.app_left .img>div:nth-child(6) span {
    animation: show 0.5s 4.1s linear forwards;
}
.line {
    width: 0;
    margin-top: 50px;
    border-top: 4px dashed #009cee;
}
@keyframes show1 {
    from {
        width: 0px;
    }
    to {
        width: 100%;
    }
}
.line {
    animation: show1 1s 0.8s linear forwards;
}