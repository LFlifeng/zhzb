.t_right {
    height: 750px;
}
.layui-form-item .title {
    margin-top: 30px;
    font-size: 20px;
    font-weight: 700;
    color:#636363;
}
.layui-form-item .layui-form-label {
    width: 114px;
    text-align: left;
}
.layui-form .layui-form-item:nth-child(2) .layui-input {
    background-color: #fff;
}
.layui-form .layui-form-item:nth-child(4) div {
    position: relative;
}
.layui-form .layui-form-item:nth-child(4) div p {
    color: #0090ec;
    width: 100px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    border-left: 1px solid #ccc;
    position: absolute;
    top: 4px;
    right: 510px;
    cursor: pointer;
}
.layui-form-item .layui-input {
    width: 300px;
    border: none;
    border-radius: 4px;
    background-color: #f1f1f1;
    vertical-align: middle;
    display: inline-block;
}
.layui-form-item button {
    margin-top: 30px;
    margin-left: 200px;
    width: 200px;
    height: 40px;
    line-height: 40px;
    text-align: center;
    font-size: 20px;
}