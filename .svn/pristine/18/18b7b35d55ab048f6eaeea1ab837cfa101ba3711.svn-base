  <script src="/demos/googlegg.js"></script>
  <script>
    $(function () {
      // 引入公共头部和底部
      $.get('../slide/header.html', function (result) {
        $result = $(result);
        $result.find('.top').appendTo('.header');
        $result.find('.logo').appendTo('.header');
      }, 'html');
      $.get('../slide/footer.html', function (result) {
        $result = $(result);
        $result.find('.footer_content').appendTo('.footer');
      }, 'html');
      // 导航定位
      // $(".nav li:eq(2) a:first").addClass("navCur")
      //验证手机 邮箱 
      $(".selyz").change(function () {
        var selval = $(this).find("option:selected").val();
        if (selval == "0") {
          $(".sel-yzsj").show()
          $(".sel-yzyx").hide()
        }
        else if (selval == "1") {
          $(".sel-yzsj").hide()
          $(".sel-yzyx").show()
        }
      })
    });
  </script>
