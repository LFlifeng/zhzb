/* 顶部搜索部分 */
.search_top {
    border: 1px solid #fff;
    margin-bottom: 20px;
    background: url(../img/smdybg.jpg) no-repeat;
    background-size: cover;
}
.search {
    margin-left: 20%;
    margin-bottom: 50px;
    margin-top: 482px;
}
.search_box {
    width: 74%;
    height: 42px;
    border-radius: 20px;
    background-color: #fff;
    text-align: center;
}
.search_box input {
    border: none;
    outline: none;
    margin-left: 20px;
    width: 80%;
    height: 40px;
    line-height: 40px;
}
.search_box img {
    width: 90px;
    margin: 5px;
}
.hot {
    margin-top: 10px;
    margin-left: 18px;
}
.hot label {
    color: #000;
}
.hot label a {
    color: #fff;
}
/* 主要内容 */
.list_content {
	width: 1200px;
	margin: 0 auto;
}
.list_search {
	padding: 20px;
	border: 1px solid #e6e6e6;
	align-items: center;
	text-align: left;
	background: #f9f9f9;
}
.pr {
	margin-top: 8px;
	height: 34px;
	line-height: 34px;
	float: left;
	width: 100px;
	text-align: center;
}
.category {
	margin-top: 8px;
	width: 1040px;
	float: left;
}
.category li {
	padding: 3px 8px;
	height: 24px;
	line-height: 24px;
	display: inline-block;
	margin-top: 6px;
}
.category_city_select {
	background: #ba3f44;
	border-radius: 3px;
}
.category_city_select a {
	color: #fff;
}
.category_leixing_select {
	background: #ba3f44;
	border-radius: 3px;
}
.category_leixing_select a {
	color: #fff;
}
.category_date_select {
	background: #ba3f44;
	border-radius: 3px;
}
.category_date_select a {
	color: #fff;
}
.category li a {
	display: inline-block;
}
.category li:hover {
	background-color: #ba3f44;
	border-radius: 3px;
}
.category li:hover a {
	color: #fff;
}
.list_list_content {
	width: 1200px;
	margin: 20px auto;
}
.list_l {
	width: 100%;
	border: 1px solid #e6e6e6;
}
.t_top {
	width: 100%;
	height: 40px;
	line-height: 40px;
	background: #f8f8f8;
}
.t_t td a {
    display: inline-block;
    width: 600px;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
    text-align: left;
    background-color: #fff;
    color: #646464;
}
.t_t td a:hover {
    color: #ba3f44;
}
.list_l table {
	width: 100%;
}
.t_t {
	text-align: center;
	height: 34px;
	line-height: 34px;
	color: #505050;
	border-bottom: 1px dashed #e6e6e6;
}
