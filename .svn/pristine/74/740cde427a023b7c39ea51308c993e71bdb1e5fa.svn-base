.head_nav .nav ul li:nth-child(5) a {
    background-color: #505050;
    color: #fff;
}
/* banner */
.banner {
    position: relative;
}
.banner div:first-child img {
    width: 100%;
}
.banner a:last-child {
    display: block;
    position: absolute;
    bottom: 50px;
    left: 815px;
    cursor: pointer;
}
.advantage .w {
    margin-top: 50px;
}
.w>h3 {
    font-size: 28px;
    color: #767170;
    font-weight: 700;
    text-align: center;
    margin-left: calc(50% - 53%);
}
hr{
    width: 78px;
    height: 4px;
    border: none;
    text-align: center;
    background-color: #d7363e;
    margin-left: 542px;
    margin-top: 20px;
    border-radius: 2px;
}
/* 优势服务 */
.advantage_content p {
    margin-top: 30px;
    color: #767170;
    font-size: 18px;
    line-height: 30px;
}
/* 三大功能 */
.advantage .under {
    margin-top: 170px;
}
.advantage .under img {
    width: 100%;
}
.advantage .info_num {
    margin-top: 100px;
    position: absolute;
}
.advantage .info_num>div {
    width: 340px;
    height: 240px;
    background-color: #d03e44;
    margin-left: 30px;
    position: relative;
}
.advantage .info_num>div p {
    margin: 100px 10px 0;
    font-size: 16px;
    font-weight: 700;
    color: #e5bdb9;
    line-height: 30px;
}
.advantage .info_num>div .num {
    position: absolute;
    left: 124px;
    top: -70px;
    width: 100px;
    height: 100px;
    line-height: 100px;
    text-align: center;
    border: 4px solid #fff;
    border-radius: 50%;
    color: #fff;
    background-color: #e96f31;
    font-size: 40px;
    font-weight: 700;
}
/* 海量数据 */
.advantage .info_img_num {
    margin: 80px 0;
}
.advantage .info_img_num>div {
    position: relative;
    margin-right: 10px;
}
.advantage .info_img_num>div>img {
    width: 224px;
}
.advantage .info_img_num>div .img_num {
    position: absolute;
    left: 60px;
    bottom: 50px;
    text-align: center;
}
.advantage .info_img_num>div .img_num p {
    color: #fff;
    font-size: 24px;
    font-weight: 700;
    margin-bottom: 10px;
    width: 104px;
    overflow: hidden;
	text-overflow:ellipsis;
    white-space: nowrap;
}
.advantage .info_img_num>div .img_num span {
    color: #fff;
    font-size: 18px;
}
/* 遮罩层 */
.mask {
    display: none;
}
.mask .left{
    width: 200px;
    height: 412px;
}
.mask .right .title {
   width: 400px;
}
.title ul {
    margin-left: 140px;
    margin-top: 20px;
}
.title ul li a {
    display: inline-block;
    width: 60px;
    height: 30px;
    line-height: 30px;
    font-size: 18px;
    color: #646464;
    text-align: center;
}
.title ul li hr {
    height: 4px;
    width: 15px;
    background-color: #d7363e;
    margin: 0;
    margin-left: 20px;
}
/* 登录部分 */
.title form:nth-child(2) {
    /* display: none; */
}
.title form:nth-child(2)>p {
    margin-left: 240px;
}
.title form:nth-child(2)>p a {
    color: #d03e44;
}
.title form input {
    width: 200px;
    height: 40px;
    line-height: 40px;
    border-radius: 5px;
    background: #e6e6e6;
    padding: 0px 10px;
    font-size: 14px;
    color: #3C3C3C;
    border: none;
    margin-top: 30px;
}
.title form:nth-child(2) .layui-form-item p {
    display: none;
    margin-left: 120px;
}
.title form:nth-child(2) .layui-form-item:nth-child(2) label {
    letter-spacing: 6px;
}
.title form:nth-child(2) .layui-form>p>a {
    margin-left: 240px;
    color: #ccc;
}
.title form:nth-child(2) .layui-form-item .layui-btn {
    width: 200px;
    margin-top: 20px;
    font-size: 16px;
    font-weight: 700;
    letter-spacing: 10px;
    background-color: #d03e44;
}
.title form:nth-child(2) .layui-form-radio {
    margin: 0;
    padding: 0;
}
.title form:nth-child(2) .layui-form-radio>i:hover, .layui-form-radioed>i {
    color: #d03e44;
}
.title form:nth-child(2) .layui-form .layui-form-item:last-child a {
    color: #d03e44;
}
/* 注册部分 */
.title form:nth-child(3) {
    display: none;
}
.title form:nth-child(3) .layui-form-item input {
    margin-top: 10px;
}
.title form:nth-child(3) .layui-form-item:nth-child(3) label {
    letter-spacing: 8px;
}
.title form:nth-child(3) .layui-form-item:nth-child(4) input {
    width: 120px;
}
.title form:nth-child(3) .layui-form-item:nth-child(4) {
    position: relative;
}
.title form:nth-child(3) .layui-form-item:nth-child(4) span {
    display: inline-block;
    border: 1px solid #35a0ee;
    border-radius: 4px;
    width: 70px;
    height: 38px;
    line-height: 38px;
    text-align: center;
    position: absolute;
    right: 90px;
    bottom: 0;
    cursor: pointer;
}
.title form:nth-child(3) .layui-form-item:nth-child(5) input {
    width: 120px;
}
.title form:nth-child(3) .layui-form-item .layui-btn {
    width: 200px;
    margin-top: 20px;
    font-size: 16px;
    font-weight: 700;
    letter-spacing: 10px;
    background-color: #d03e44;
}