body {
    background-color: #fff;
}
.zh_container {
    background-color: #f1f1f1;
}
.zh_topBar {
    background-color: #fff;
}

.zh_topBar a {
    color: #ba3f44;
    height: 50px;
    line-height: 50px;
    font-size: 15px;
    margin: 0 10px;
}

.zh_topBar img {
    width: 15px;
}

.zh_topBar span {
    color: #505050;
    font-size: 17px;
}

/*  */
.mui-scroll {
    width: auto;
}
.zh_content {
    /* border-top: 1px solid #fff;  */
}
/* 会员等级 */
.m_member {
    margin-top: 50px;
    width: 600px;
    height: 130px;
    background-color: #515151;
    position: relative;
    border-top: 1px solid #fff;
}
.m_member .m_title {
    margin-left: 20px;
    color: #cdcdcd;
    font-size: 16px;
    margin-top: 10px;
}
.m_member .m_content {
    position: absolute;
    top: 50px;
    width: 600px;
}
.m_content img {
    width: 124px;
}
.m_content .img {
    margin-left: 20px;
}
.m_content .img.active {
    border: 3px solid #d0d0d0;
    border-radius: 11px;
    box-shadow: 0 4px 30px #000;
}
/* 会员购买 */
.pay {
    margin-top: 300px;
    text-align: center;
    background-color: #fff;
    border-bottom: 1px solid #fff;
}
.b_pay {
    width: 200px;
    height: 36px;
    line-height: 36px;
    text-align: center;
    font-size: 16px;
    color: #fff;
    background-color: #f1504e;
    border-radius: 18px;
    margin-left: 23%;
}
.o_pay {
    font-size: 16px;
    color: #cfa56b;
    margin-top: 10px;
    margin-bottom: 20px;
}
/* 七大权益 */
.rights {
    margin-top: 10px;
    background-color: #fff;
    border-top: 1px solid #fff;
}
.rights .r_title {
    margin-top: 10px;
    margin-left: 10px;
    color: #cfa669;
    font-size: 16px;
    font-weight: 700;
}
.rights .right {
    margin-top: 10px;
    margin-left: 50px;
    text-align: center;
}
.rights .right img {
    width: 48px;
}
.rights .right div {
    color: #787878;
    font-size: 15px;
    margin: 10px 0;
}

