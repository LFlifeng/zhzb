.head_nav .nav ul li:nth-child(5) a {
    background-color: #505050;
    color: #fff;
}
/* 最新消息 */
.new_infos {
    margin-top: 30px;
}
.release {
    border: 1px solid #e2e2e2;
    height: 360px;
    margin-right: 10px;
	overflow: hidden;
}
.release .release_title {
    font-size: 16px;
	height: 40px;
	background: #f4f4f4;
	text-align: left;
	padding-left: 10px;
	line-height: 40px;
	color: #646464;
}
.release .release_title_content {
    padding: 10px;
	height: 320px;
}
.release .release_title_content li a {
    display: inline-block;
	width: 230px;
	color: #646464;
	line-height: 34px;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
	text-align: left;
}
.img {
    margin-right: 10px;
}
/* 优秀供应商 */
.supplier {
    margin-top: 30px;
}
.supplier_info {
    border: 1px solid #e2e2e2;
    width: 795px;
    margin-right: 10px;
}
.supplier_info .supplier_info_title {
    font-size: 16px;
	height: 40px;
	background: #f4f4f4;
	text-align: left;
	padding-left: 10px;
	line-height: 40px;
	color: #646464;
}
.supplier_info_list {
    border-bottom: 1px solid #e2e2e2;
}
.supplier_info_list:last-child {
    border: none;
}
.supplier_info_list .supplier_img,
.supplier_info_list .supplier_info_business{
    margin: 30px 0px 30px 20px;
}
.supplier_info_list .supplier_img img {
    width: 80px;
}
.supplier_info_business h3 {
    font-size: 18px;
    color: #646464;
}
.supplier_info_business .supplier_address {
    margin-top: 5px;
}
.supplier_info_business .supplier_address span:nth-child(3) {
    margin-left: 30px;
}
.supplier_info_business .business {
    margin-top: 5px;
}
.supplier_info_business .business span:nth-child(2) {
    display: inline-block;
    width: 500px;
    overflow: hidden;
	text-overflow:ellipsis;
    white-space: nowrap;
    vertical-align: top;
}
.supplier_info_list>div.r {
    margin-top: 30px;
    margin-right: 30px;
}
.supplier_info_list>div.r a {
    color: #35a0ee;
}
.ranking {
    border: 1px solid #e2e2e2;
    height: 460px;
}
.ranking .ranking_title {
    font-size: 16px;
	height: 40px;
	background: #f4f4f4;
	text-align: left;
	padding-left: 10px;
	line-height: 40px;
	color: #646464;
}
.ranking .ranking_mc li div {
    display: inline-block;
    width: 40px;
    height: 34px;
    line-height: 34px;
    text-align: center;
    vertical-align: top;
}
.ranking .ranking_mc li a {
    display: inline-block;
    width: 200px;
    height: 34px;
	color: #646464;
	line-height: 34px;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
	text-align: left;
}
.mc .c1 img {
    width: 339px;
    margin-top: 20px;
}
/* 遮罩层 */
.mask {
    /* display: none; */
}
.mask .left{
    width: 200px;
    height: 412px;
}
.mask .right .title {
   width: 380px;
}
.title ul {
    margin-left: 140px;
    margin-top: 20px;
}
.title ul li a {
    display: inline-block;
    width: 60px;
    height: 30px;
    line-height: 30px;
    font-size: 18px;
    color: #646464;
    text-align: center;
}
.title ul li hr {
    height: 4px;
    width: 15px;
    background-color: #d7363e;
    margin: 0;
    margin-left: 20px;
}
/* 登录部分 */
.title form:nth-child(2) {
    /* display: none; */
}
.title form:nth-child(2)>p {
    margin-left: 240px;
}
.title form:nth-child(2)>p a {
    color: #d03e44;
}
.title form input {
    width: 200px;
    height: 40px;
    line-height: 40px;
    border-radius: 5px;
    background: #e6e6e6;
    padding: 0px 10px;
    font-size: 14px;
    color: #3C3C3C;
    border: none;
    margin-top: 30px;
}
.title form:nth-child(2) .layui-form-item p {
    display: none;
    margin-left: 120px;
}
.title form:nth-child(2) .layui-form-item:nth-child(2) label {
    letter-spacing: 6px;
}
.title form:nth-child(2) .layui-form>p>a {
    margin-left: 240px;
    color: #ccc;
}
.title form:nth-child(2) .layui-form-item .layui-btn {
    width: 200px;
    margin-top: 20px;
    font-size: 16px;
    font-weight: 700;
    letter-spacing: 10px;
    background-color: #d03e44;
}
.title form:nth-child(2) .layui-form-radio {
    margin: 0;
    padding: 0;
}
.title form:nth-child(2) .layui-form-radio>i:hover, .layui-form-radioed>i {
    color: #d03e44;
}
.title form:nth-child(2) .layui-form .layui-form-item:last-child a {
    color: #d03e44;
}
/* 注册部分 */
.title form:nth-child(3) {
    display: none;
}
.title form:nth-child(3) .layui-form-item input {
    margin-top: 10px;
}
.title form:nth-child(3) .layui-form-item:nth-child(3) label {
    letter-spacing: 8px;
}
.title form:nth-child(3) .layui-form-item:nth-child(4) input {
    width: 120px;
}
.title form:nth-child(3) .layui-form-item:nth-child(4) {
    position: relative;
}
.title form:nth-child(3) .layui-form-item:nth-child(4) span {
    display: inline-block;
    border: 1px solid #35a0ee;
    border-radius: 4px;
    width: 70px;
    height: 38px;
    line-height: 38px;
    text-align: center;
    position: absolute;
    right: 70px;
    bottom: 0;
    cursor: pointer;
}
.title form:nth-child(3) .layui-form-item:nth-child(5) input {
    width: 120px;
}
.title form:nth-child(3) .layui-form-item .layui-btn {
    width: 200px;
    margin-top: 20px;
    font-size: 16px;
    font-weight: 700;
    letter-spacing: 10px;
    background-color: #d03e44;
}