.main {
	width: 100%;
	height: 180px;
	background: #fafafa;
	border-bottom: 1px solid #e6e6e6;
}

.main_content {
	width: 1200px;
	margin: 0 auto;
}

.main_content h3 {
	font-size: 20px;
	font-weight: 600;
	color: #c6c6c6;
	padding-top: 20px;
	margin-bottom: 10px;
}

