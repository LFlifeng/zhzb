}

/*主体*/

.content {
	width: 1200px;
	margin: 10px auto;
}

.sub_menu {
	width: 220px;
	height: 480px;
	padding: 15px;
	background: #008FED;
	position: relative;
}

.sub_menu_more {
	position: absolute;
	bottom: 10px;
	right: 10px;
	color: #ffffff;
}

.sub_menu_more a:hover{
	
	color:#0A7FCB;
	
}
	
.sub_menu span {
	font-size: 14px;
	color: #e7f3fb;
	height: 24px;
	line-height: 24px;
	display: inline-block;
}

.sub_menu li {
	border-bottom: 1px #C6C6C6 dashed;
}

.sub_menu li h3 {
	margin-top: 10px;
	font-size: 14px;
	color: #ffffff;
}

.sub_menu p {
	padding: 2px 5px;
	height: 30px;
	line-height: 30px;
	display: inline-block;
}

.lb a {
	color: #ffffff;
}

.more {
	color: #ffffff;
}

