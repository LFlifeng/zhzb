	border: 1px solid #ccc;
	margin-top: 20px;
}
.bidding_info>div {
	text-align: center;
	margin-top: 20px;
	padding-bottom: 10px;
	border-bottom:1px dotted #ccc;
}
.bidding_info>div:last-child {
	border: none;
}
.bidding_info>div>p {
	color: #909090;
	font-size: 16px;
	font-weight: 700;
	margin-bottom: 10px;
}
.bidding_info>div>span:nth-child(2) {
	font-size: 18px;
	color: #eb6869;
	font-weight: 700;
}
.send {
	margin-top: 50px;
	background: #ea9961;
	border-radius: 4px;
	position: relative;
}
.send .fa-pencil{
	position: absolute;
	top: 5px;
	left: 50px;
}
.send .send_btn {
	border: none;
	background: none;
