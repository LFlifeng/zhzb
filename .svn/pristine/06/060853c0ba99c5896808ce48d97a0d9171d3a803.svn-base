	height: 50px;
}
.suspension .conslut .suspension-list {
	position: absolute;
	left: -200px;
	bottom: 0;
	background: #fff;
	box-shadow: #c8c8c8 0 0 10px;
}
.suspension-list-content li div {
	margin: 0 10px;
	padding: 20px 0px; 
	width: 180px;
	border-bottom: 1px solid #e2e2e2;
	text-align: center;
	position: relative;
}
.suspension-list-content li div::before {
	display: block;
	content: "";
	width: 50px;
	height: 50px;
	position: absolute;
	left: -10px;
} 
.suspension-list-content li:first-child div::before {
	background: url(../img/dianhua.png) no-repeat center;
}
.suspension-list-content li:nth-child(2) div::before {
	background: url(../img/qq.png) no-repeat center;
}
.suspension-list-content li:last-child div::before {
	background: url(../img/yijian.png) no-repeat center;
}
.suspension-list-content li:last-child div {
	border: none;
}
.suspension-list-content li div p:last-child {
	color: #ea9961;
}
.suspension-list-content li:last-child div p:last-child {
	color: #e2e2e2;
}
.suspension .cart {
	display: block;
	position: relative;
	width: 50px;
	height: 50px;
	background: url(../img/qrcode.png) no-repeat center;
}
.suspension .cart .pic {
	position: absolute;
	left: -79px;
	bottom: 0;
}
.suspension .cart .pic img {
	width: 80px;
}
.suspension .back-top{
	display: block;
	position: relative;
	width: 50px;
	height: 50px;
	background: url(../img/top.jpg) no-repeat center;
}