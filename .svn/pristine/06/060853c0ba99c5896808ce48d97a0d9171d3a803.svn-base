	border-bottom: 1px #e2e2e2 solid;
}
.user_fun ul li a::before {
	display: block;
	content: "";
	width: 28px;
	height: 28px;
	position: absolute;
	top: -40px;
	left: 14px;
	background: no-repeat center;
}
