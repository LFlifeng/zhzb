	border-right: none;
}
.user_fun ul li:nth-child(4) {
	border-bottom: none;
}
.user_fun ul li:nth-child(5) {
	border-bottom: none;
}
.user_fun ul li:nth-child(6) {
	border-right: none;
	border-bottom: none;
}
.user_fun ul li:first-child a::before {
	background-image: url("../img/indexicon1.png");
}
.user_fun ul li:nth-child(2) a::before {
	background-image: url("../img/indexicon2.png");
}
.user_fun ul li:nth-child(3) a::before {
	background-image: url("../img/indexicon3.png");
}
.user_fun ul li:nth-child(4) a::before {
	background-image: url("../img/indexicon4.png");
}
.user_fun ul li:nth-child(5) a::before {
	background-image: url("../img/indexicon5.png");
}
.user_fun ul li:nth-child(6) a::before {
	background-image: url("../img/indexicon6.png");
}
