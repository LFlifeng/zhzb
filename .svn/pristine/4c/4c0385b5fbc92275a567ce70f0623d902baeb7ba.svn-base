body {
    background-color: #fff;
}
.mui-table-view {
    border: 1px solid #fff;
}
.mui-table-view:before {
    background-color: #fff;
}
.zh_topBar {
    background-color: #fff;
}
.zh_topBar a {
    color: #ba3f44;
    height: 50px;
    line-height: 50px;
    font-size: 15px;
    margin: 0 10px;
}
.zh_topBar img {
    width: 15px;
}
.zh_topBar span {
    color: #505050;
    font-size: 17px;
}
.mui-bar-nav.mui-bar .mui-icon {
    margin: 0;
    padding: 0;
    color: #000;
}
/*  */
.zh_content {
    /* border-top: 1px solid #fff;  */
}
.zh_content .alone {
    margin-top: 45px;
}
.zh_content .mui-table-view p {
    margin-left: 5%;
    margin-top: 5px;
    margin-bottom: 0;
}
.mui-table-view-cell div {
    font-size: 17px;
    color: #505050;
}
.mui-table-view-cell .edit {
    font-size: 15px;
    color: #c8c8c8;
}
.mui-table-view-cell .del {
    font-size: 15px;
    color: #c8c8c8;
}
.zh_content .mui-table-view>div .addA {
    margin-right: 4%;
    color: #ba3f44;
}
.mui-backdrop {
    z-index: 998;
    display:block;
}
/* 类型选项 */
.zh_content .mui-table-view .type {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-bottom: 5px;
}
.zh_content .mui-table-view .type p {
    width: 100px;
    height: 30px;
    border: 1px solid #b4b4b4;
    color: #b4b4b4;
    font-size: 14px;
    text-align: center;
    line-height: 30px;
    border-radius: 15px;
    margin-right: 15px;
    margin-bottom: 0;
}
.zh_content .mui-table-view .type p.active {
    background-color: #348eea;
    color: #fff;
    border: #348eea;
}